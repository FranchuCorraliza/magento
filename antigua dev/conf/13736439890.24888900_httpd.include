</IfModule>

    ServerAdmin  "gcaceres@magento-spain.com"

	DocumentRoot "/var/www/vhosts/elitestore.es/httpdocs"
    CustomLog /var/www/vhosts/elitestore.es/statistics/logs/access_ssl_log plesklog
